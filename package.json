    "analyze": "vite build --mode analyze",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "server": "tsx server.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run server\"",
    "db:setup": "tsx src/lib/db/setup.js"
  },
